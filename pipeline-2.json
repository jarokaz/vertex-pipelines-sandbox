{
  "pipelineSpec": {
    "components": {
      "comp-test-op": {
        "executorLabel": "exec-test-op",
        "inputDefinitions": {
          "parameters": {
            "location": {
              "type": "STRING"
            },
            "machine_type": {
              "type": "STRING"
            },
            "project": {
              "type": "STRING"
            },
            "runners_image": {
              "type": "STRING"
            },
            "sleep_time": {
              "type": "INT"
            }
          }
        },
        "outputDefinitions": {
          "parameters": {
            "gcp_resources": {
              "type": "STRING"
            }
          }
        }
      }
    },
    "deploymentSpec": {
      "executors": {
        "exec-test-op": {
          "container": {
            "args": [
              "--project",
              "{{$.inputs.parameters['project']}}",
              "--location",
              "{{$.inputs.parameters['location']}}",
              "--runners_image",
              "{{$.inputs.parameters['runners_image']}}",
              "--sleep_time",
              "{{$.inputs.parameters['sleep_time']}}",
              "--machine_type",
              "{{$.inputs.parameters['machine_type']}}",
              "--gcp_resources",
              "{{$.outputs.parameters['gcp_resources'].output_file}}",
              "--executor_input",
              "{{$}}"
            ],
            "command": [
              "python3",
              "-u",
              "-m",
              "launcher.test_task"
            ],
            "image": "gcr.io/jk-mlops-dev/app:latest"
          }
        }
      }
    },
    "pipelineInfo": {
      "name": "pipeline-2"
    },
    "root": {
      "dag": {
        "tasks": {
          "test-op": {
            "cachingOptions": {
              "enableCache": true
            },
            "componentRef": {
              "name": "comp-test-op"
            },
            "inputs": {
              "parameters": {
                "location": {
                  "runtimeValue": {
                    "constantValue": {
                      "stringValue": "us-central1"
                    }
                  }
                },
                "machine_type": {
                  "runtimeValue": {
                    "constantValue": {
                      "stringValue": "n1-standard-8"
                    }
                  }
                },
                "project": {
                  "runtimeValue": {
                    "constantValue": {
                      "stringValue": "jk-mlops-dev"
                    }
                  }
                },
                "runners_image": {
                  "runtimeValue": {
                    "constantValue": {
                      "stringValue": "gcr.io/jk-mlops-dev/app"
                    }
                  }
                },
                "sleep_time": {
                  "runtimeValue": {
                    "constantValue": {
                      "intValue": "360"
                    }
                  }
                }
              }
            },
            "taskInfo": {
              "name": "test-op"
            }
          }
        }
      }
    },
    "schemaVersion": "2.0.0",
    "sdkVersion": "kfp-1.8.12"
  },
  "runtimeConfig": {}
}